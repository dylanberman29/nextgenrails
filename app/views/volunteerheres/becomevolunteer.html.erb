<section class="signupsec">
  <p id="notice"><%= notice %></p>

    <legend class="joinusleg"> Join us by appying below.</legend>

<%= form_with model: @volunteer, class:'signupform' do |f| %>

<%= f.label :first_name, class: 'signupformlabel' %>
<%= f.text_field :first_name, class:'signupformanswer', id:'firstnameField'%>

<%= f.label :last_name, class: 'signupformlabel'%>
